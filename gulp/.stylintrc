{
  "blocks": false,
  "brackets": false,
  "colons": false,
  "colors": {
    "expect": "always"
  },
  "commaSpace": "always",
  "commentSpace": "always",
  "cssLiteral": "never",
  "depthLimit": false,
  "duplicates": true,
  "efficient": "always",
  "extendPref": false,
  "globalDupe": false,
  "indentPref": 4,
  "leadingZero": "always",
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": false,
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "none": false,
  "noImportant": false,
  "parenSpace": "always",
  "placeholders": false,
  "prefixVarsWithDollar": "always",
  "quotePref": "single",
  "semicolons": false,
  "stackedProperties": "never",
  "trailingWhitespace": "never",
  "universal": false,
  "valid": true,
  "zeroUnits": false,
  "zIndexNormalize": false
}